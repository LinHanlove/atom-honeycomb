{
  "name": "Atom Honeycomb",
  "displayName": "Atom Honeycomb",
  "version": "0.0.1",
  "description": "Atom Honeycomb: Streamline your browsing experience with quick search, cache clearing, screenshot capture, and more — all in one convenient extension.",
  "author": "LinHan",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "@reduxjs/toolkit": "^2.2.7",
    "atom-tools": "^0.5.91",
    "cropperjs": "^1.6.2",
    "html2canvas": "^1.4.1",
    "plasmo": "0.89.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@iconify/react": "^5.0.2",
    "@plasmohq/storage": "^1.12.0",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "<all_urls>"
    ],
    "permissions": [
      "topSites",
      "scripting",
      "activeTab",
      "action",
      "tabs",
      "storage",
      "contextMenus",
      "notifications"
    ],
    "commands": {
      "jsonFormatter": {
        "suggested_key": {
          "default": "Alt+J",
          "mac": "Alt+J",
          "windows": "Alt+J"
        },
        "description": "Json Formatter"
      },
      "openGitHubDev": {
        "suggested_key": {
          "default": "Alt+G",
          "mac": "Alt+G",
          "windows": "Alt+G"
        },
        "description": "激活githubDev"
      },
      "refresh": {
        "suggested_key": {
          "default": "Alt+C",
          "mac": "Alt+C",
          "windows": "Alt+C"
        },
        "description": "强刷"
      },
      "areaScreenshot": {
        "suggested_key": {
          "default": "Alt+A",
          "mac": "Alt+A",
          "windows": "Alt+A"
        },
        "description": "区域截图"
      }
    },
    "$schema": "https://raw.githubusercontent.com/PlasmoHQ/bpp/v3/keys.schema.json"
  }
}